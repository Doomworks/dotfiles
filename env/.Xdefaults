! black
*.color0:       #212121
*.color8:       #3a3a3a

! red
*.color1:       #c42f17
*.color9:       #da392d

! green
*.color2:       #51a74b
*.color10:      #97cc5a

! yellow
*.color3:       #dbba0f
*.color11:      #f3e367

! blue
*.color4:       #2c6cad
*.color12:      #5596d3

! magenta
*.color5:       #a16ebb
*.color13:      #bc93b6

! cyan
*.color6:       #09c3c7
*.color14:      #2fd5d7

! white
*.color7:       #eeeeee
*.color15:      #f1efee
