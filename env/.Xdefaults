XTerm*termName: xterm-256color

URxvt*scrollBar: false
URxvt*internalBorder: 1
URxvt*font: -*-tamzenforpowerline-medium-*-*-*-14-*-*-*-*-*-*-*, \
            -wuncon-siji-medium-r-normal--10-*-*-*-*-*-*-*, \
            xft:FontAwesome-11,
URxvt*letterSpace: 0
URxvt*cursorBlink: true


*.foreground:   #a8a19f
*.background:   #212121
